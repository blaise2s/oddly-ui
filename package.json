{
  "name": "oddly-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "yarn@4.5.1",
  "lint-staged": {
    "*.{css,scss}": "stylelint --fix --allow-empty-input",
    "*.{tsx,jsx}": "eslint --cache --fix --max-warnings 0",
    "**/*": "prettier --write --ignore-unknown"
  },
  "scripts": {
    "build": "tsc -b && vite build",
    "dev": "vite",
    "lint": "eslint .",
    "prepare": "husky",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "3.3.3",
    "stylelint": "^16.10.0",
    "stylelint-config-standard": "^36.0.1",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.12.2",
    "vite": "^5.4.10"
  }
